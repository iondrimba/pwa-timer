  "dependencies": {
    "@types/jest": "24.0.25",
    "@types/node": "12.12.21",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-test-renderer": "^16.9.1",
    "@types/styled-components": "^4.4.2",
    "history": "^4.10.1",
    "jest-styled-components": "^6.3.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "react-spring": "^8.0.27",
    "react-test-renderer": "^16.12.0",
    "styled-components": "^4.4.1",
    "styled-normalize": "^8.0.6",
    "typescript": "3.7.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --transformIgnorePatterns 'node_modules/react-spring'",
    "coverage": "react-scripts test --coverage --watchAll=false --transformIgnorePatterns 'node_modules/react-spring'",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "resetMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/react-app-env.d.ts",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "babel-plugin-styled-components": "^1.10.6"
  }
}
